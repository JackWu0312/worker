.home {
  background-color: #F8F8F8;
  padding-bottom: 150rpx;
}

.todo {
  margin-top: 20rpx;
  box-shadow: 0rpx 4rpx 10rpx 0rpx #EBEBEB;

}

.title {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding-right: 40rpx;
  box-sizing: border-box;
  background-color: #fff;
  height: 80rpx;
  width: 750rpx;
}

.title view:nth-child(1) {
  display: flex;
  height: 80rpx;
  align-items: center;
}

.title view:nth-child(1)>view:nth-child(1) {
  width: 8rpx;
  height: 50rpx;
  background: #36CA8F;
  margin-right: 30rpx;
}

.title view {
  font-size: 27rpx;
  font-weight: 400;
  color: #36CA8F;
}

.roomState {
  width: 750rpx;
  height: 180rpx;
  background-color: #fff;
  border-top: 1rpx solid #F4F4F4;
  display: flex;
  padding: 0 35rpx;
  box-sizing: border-box;
  justify-content: space-between;
  align-items: center;
}

.roomCollection {
  display: flex;
  flex-direction: column;

}

.roomCollection>view:nth-child(1) {
  font-size: 30rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #000000;
}

.roomCollection>view:nth-child(2) {
  margin-top: 20rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #D9D9D9;
}

.roomStateItem {
  display: flex;
  flex-direction: column;

}

.roomStateItem view:nth-child(1) {
  font-size: 30rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  text-align: center;
}

.roomStateItem view:nth-child(2) {
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #D9D9D9; 
  margin-top: 35rpx;
}

.managementContent {
  background-color: #fff;
  height: 240rpx;
  border-top: 1rpx solid #F4F4F4;
  display: flex;
}
.newwith{
  width: 250rpx!important;
}
.managementItem {
  width: 187.5rpx;
  border-right: 1rpx solid #F4F4F4;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  align-items: center;
  position: relative;
}

.managementItem >image:nth-child(1) {
  width: 64rpx;
  margin-top: 45rpx;
  height: 64rpx;
}
.hot{
  position: absolute;
  width: 50rpx;
  height: 50rpx;
  top: 0;
  right: 0;
}
.managementItem view:nth-child(2) {
  font-size: 34rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #000000;
  margin-top: 26rpx;
}

.managementItem view:nth-child(3) {
  font-size: 20rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  margin-top: 12rpx;
  color: #ACACAC;
}

.managementItem:nth-last-child(1) {
  border-right: none;

}