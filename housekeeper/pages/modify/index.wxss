page{
  width: 100%;
  height: 100%;
}
.modify{
  height: 100%;
  background-color: #fff;
  background-color: #F8F8F8;
}

.box{ 
  padding-top: 130rpx;
}


.item {
  height: 90rpx;
  background-color: #fff;
  padding: 0 30rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-bottom: 1rpx solid #F4F4F4;
}

.item>view:nth-child(1) {
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #000000;
}

.item>view:nth-child(1)>text:nth-child(2) {
  margin-left: 20rpx;
  color: #00C77A;
  padding-top: 15rpx;
}

.muban {
  display: flex;
  align-items: center;
}

.muban view:nth-child(1) {
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #A9A9A9;
}

.muban image {
  width: 21rpx;
  height: 21rpx;
  margin-left: 15rpx;
}

.select {
  width: 140rpx;
  height: 64rpx;
  border-radius: 10rpx;
  box-sizing: border-box;
  border: 2rpx solid #55D970;
  display: flex;
}
.btn{
  position:absolute ;
  left: 30rpx;
  bottom: 100rpx;
  width: 690rpx;
  height: 90rpx;
  border-radius: 45rpx;
  line-height: 90rpx;
  text-align: center;
  font-size: 30rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #FFFFFF;
  background: linear-gradient(270deg, #3BCC8A 0%, #5BDD66 100%);
}
.dete{
  color: #584444!important;
}