.electronics {
  background-color: #F8F8F8;
  min-height: 100%;
}


.tab {
  width: 750rpx;
  height: 70rpx;
  background: linear-gradient(270deg, #3BCC8A 0%, #5BDD66 100%);
  display: flex;
  align-items: center;
  position: fixed;
  left: 0;
  top: 128rpx;
  z-index: 2;
  box-sizing: border-box;
  padding: 0 150rpx;
  justify-content: space-between;
}

.tab view {
  font-size: 30rpx;
  height: 66rpx;
  line-height: 66rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #8AEEB3;
}

.tabActive {
  color: #fff !important;
  border-bottom: 2rpx solid #fff;
}

.list {
  padding-top: 198rpx;
  padding-bottom: 30rpx;
}



.item {
  padding: 10rpx 30rpx 30rpx 30rpx;
  margin-bottom: 20rpx;
  background-color: #fff;
}

.itemtit {

  font-size: 32rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #000000;
}

.info {
  display: flex;
  align-items: center;
  font-size: 26rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #ACACAC;
  margin-top: 10rpx;
  justify-content: space-between;

}
.info view:nth-child(2){
  color: #3BCC8A;
}

.info image{
  width: 45rpx;
  height: 45rpx;
}
.text {
  font-size: 26rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #ACACAC;
  margin-top: 10rpx;
  display: flex;
  justify-content: space-between;
}



.itembtn {
  display: flex;
  border-top: 1rpx solid #F8F8F8;
  margin-top: 20rpx;
  justify-content:space-around;
} 

.itembtn>view {
  /* width: 350rpx; */
  height: 90rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 30rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #36CA8F;
}

.itembtn>view>image {
  width: 32rpx;
  height: 32rpx;
  margin-right: 20rpx;
}

.noData {
  font-size: 30rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #000000;
  text-align: center;
  padding-top: 300rpx;
}

.bgck{
  display: flex;
  align-items: center;
  font-size: 26rpx;
    font-family: PingFangSC-Regular, PingFang SC;
    font-weight: 400;
    color: #ACACAC;
    margin-top: 10rpx;
}
.bgck .tips{
  color: #fff;
  border-radius: 4rpx;
  padding: 0 8rpx;
  /* background-color: #14affe; */
  font-size: 22rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  margin-right: 30rpx;
}

.noData {
  font-size: 30rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #000000;
  text-align: center;
  padding-top: 300rpx;
}