.together{
  background-color: #F8F8F8;
  height: 100%;
}

.box{
  padding-top: 138rpx;
}
.item {
  height: 90rpx;
  background-color: #fff;
  padding: 0 30rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-bottom: 1rpx solid #F4F4F4;
}

.item>view:nth-child(1) {
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #000000;
}

.item>view:nth-child(1)>text:nth-child(2) {
  margin-left: 20rpx;
  color: #00C77A;
  padding-top: 15rpx;
}

.item>view:nth-child(2)>input {
  height: 90rpx;
  width: 300rpx;
  text-align: right;
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #000000;
}

.place {
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #A9A9A9;
}

.sex {
  width: 130rpx;
  height: 60rpx;
  border-radius: 10rpx;
  border: 2rpx solid #55D970;
  display: flex;
}

.sex view {
  width: 66rpx;
  line-height: 60rpx;
  text-align: center;
  font-size: 26rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #55D970;
}

.active {
  background: #55D970;
  color: #FFFFFF !important;
}

.prove{
  padding: 0 30rpx;
  padding-bottom: 15rpx;
  background-color: #fff;
  height: 300rpx;
}

.prove view:nth-child(1){
  height: 100rpx;
  font-size: 28rpx;
  line-height: 100rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #000000;
}
.prove view:nth-child(2){
  display: flex;
  justify-content: space-between;
}
.prove view:nth-child(2) image{
  width: 160rpx;
  height: 160rpx;
}

.btn {
  width: 750rpx;
  height: 200rpx;
  background-color: #F8F8F8;
  box-sizing: border-box;
  padding: 0 30rpx;
  display: flex;
  align-items: center;
}

.btn view {
  width: 100%;
  height: 90rpx;
  border-radius: 45rpx;
  line-height: 90rpx;
  text-align: center;
  font-size: 30rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #FFFFFF;
  background: linear-gradient(270deg, #3BCC8A 0%, #5BDD66 100%);
}